{
  "name": "geoaudit",
  "version": "1.0.1",
  "description": "Lighthouse for AI. Audit any webpage for generative engine readiness. 7 categories, 30+ factors, research-backed scoring. Deterministic, engine-agnostic, zero API keys.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "bin": {
    "geoaudit": "./bin/geoaudit.js"
  },
  "files": [
    "dist",
    "!dist/*.map",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "tsc --noEmit",
    "format:check": "prettier --check .",
    "ci": "npm run format:check && npm run lint && npm run test:coverage && npm run build",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agencyenterprise/geoaudit.git"
  },
  "homepage": "https://github.com/agencyenterprise/geoaudit#readme",
  "bugs": {
    "url": "https://github.com/agencyenterprise/geoaudit/issues"
  },
  "author": "Agency Enterprise",
  "license": "MIT",
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "keywords": [
    "geo",
    "geoaudit",
    "generative-engine-optimization",
    "ai-readiness",
    "ai-audit",
    "llm",
    "chatgpt",
    "claude",
    "perplexity",
    "gemini",
    "ai-overview",
    "content-audit",
    "content-analysis",
    "seo",
    "nlp",
    "structured-data",
    "schema-org",
    "json-ld",
    "llms-txt",
    "robots-txt",
    "lighthouse",
    "cli",
    "audit",
    "readability",
    "web-audit"
  ],
  "dependencies": {
    "chalk": "^5.6.2",
    "cheerio": "^1.2.0",
    "commander": "^14.0.3",
    "compromise": "^14.14.5",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@vitest/coverage-v8": "^4.0.18",
    "prettier": "^3.8.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
