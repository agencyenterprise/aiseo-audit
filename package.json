{
  "name": "geoaudit",
  "version": "0.1.2",
  "description": "Lighthouse for AI. Audit any webpage for generative engine readiness. 7 categories, 30+ factors, research-backed scoring. Deterministic, engine-agnostic, zero API keys.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "bin": {
    "geoaudit": "./bin/geoaudit.js"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agencyenterprise/geoaudit.git"
  },
  "homepage": "https://github.com/agencyenterprise/geoaudit#readme",
  "bugs": {
    "url": "https://github.com/agencyenterprise/geoaudit/issues"
  },
  "author": "Agency Enterprise",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "geo",
    "geoaudit",
    "generative-engine-optimization",
    "ai-readiness",
    "ai-audit",
    "llm",
    "chatgpt",
    "claude",
    "perplexity",
    "gemini",
    "ai-overview",
    "content-audit",
    "content-analysis",
    "seo",
    "nlp",
    "structured-data",
    "schema-org",
    "json-ld",
    "llms-txt",
    "robots-txt",
    "lighthouse",
    "cli",
    "audit",
    "readability",
    "web-audit"
  ],
  "dependencies": {
    "axios": "^1.7.0",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0",
    "commander": "^12.1.0",
    "compromise": "^14.14.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "tsup": "^8.1.0",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  }
}
